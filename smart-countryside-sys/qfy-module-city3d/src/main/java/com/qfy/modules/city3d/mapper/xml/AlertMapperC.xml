  </sql>
  <delete id="batchDelete" parameterType="java.util.List" >
    delete from alert where id in
    <foreach collection="list" item="item" separator="," open="(" close=")">
    	#{item,jdbcType=VARCHAR}
    </foreach>
  </delete>
</mapper>

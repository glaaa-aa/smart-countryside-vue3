  <select id="queryCompanyData" resultMap="BaseResultMap" parameterType="com.qfy.modules.city3d.po.Company" >
  	select c.*,a.* from company c left join address a on c.add_id = a.add_id
  	where COALESCE(name,'') like concat('%',#{name},'%')
  	and COALESCE(organization,'') like concat('%',#{organization},'%')
  	and COALESCE(leader,'') like concat('%',#{leader},'%')
  	and COALESCE(legal_man,'') like concat('%',#{legalMan},'%')
  	and COALESCE(address,'') like concat('%',#{address},'%')
  	and COALESCE(industry_type,'') like concat('%',#{industryType},'%')
  	and COALESCE(main_business,'') like concat('%',#{mainBusiness},'%')
  	and COALESCE(second_business,'') like concat('%',#{mainBusiness},'%')
  	order by c.name
  </select>
  <select id="selectCompanyById" resultMap="BaseResultMap">
  	select c.*, a.* from company c left join address a on c.add_id = a.add_id where c.company_id = #{companyId}
  </select>
  <select id="getCompanyByAddId" resultMap="BaseResultMap">
  	select c.*, a.* from company c left join address a on c.add_id = a.add_id where c.add_id = #{addId}
  </select>
  <select id="getCompanyByComName" resultMap="BaseResultMap">
  	select c.*, a.* from company c left join address a on c.add_id = a.add_id where
  	c.community = #{community}
  </select>

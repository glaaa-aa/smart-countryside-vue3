  <!--用于历史轨迹回放  查一个表 -->
  <select id="selectGpsInfosInOneDay" parameterType="java.util.HashMap" resultType ="com.qfy.modules.city3d.po.GpsHisDataInfo">
	select * from ${tableName1} a
		<where>
			<if test="simNo!=null">
            	a.simNo = #{simNo, jdbcType=VARCHAR}
            </if>
			<if test="startTime!=null">
				<![CDATA[and a.sendTime >= #{startTime,jdbcType=VARCHAR]]>
			</if>
			<if test="endTime!=null">
			  	<![CDATA[and a.sendTime <= #{endTime,jdbcType=VARCHAR}]]>
			</if>
			and a.velocity >= #{velocity,jdbcType=INTEGER}
		</where>order by a.sendTime
  </select>
  <!--用于历史轨迹回放 跨两个表 -->
  <select id="selectGpsInfosIn2Days" parameterType="java.util.HashMap" resultType ="com.qfy.modules.city3d.po.GpsHisDataInfo">
	select d.* from (select * from ${tableName1} a
		<where>
			<if test="simNo!=null">
            	a.simNo = #{simNo, jdbcType=VARCHAR}
            </if>
			<if test="startTime!=null">
				<![CDATA[and a.sendTime >= #{startTime,jdbcType=VARCHAR]]>
			<!-- and a.updatedate>=#{startTime,jdbcType=VARCHAR} -->
			</if>
			<if test="endTime!=null">
			  <![CDATA[and a.sendTime <= #{endTime,jdbcType=VARCHAR}]]>
			</if>
			and a.velocity >= #{velocity,jdbcType=INTEGER}
		</where>
		 union all  select * from ${tableName2} as b
		 <where>
		 	<if test="simNo!=null">
            	b.simNo = #{simNo, jdbcType=VARCHAR}
            </if>
            <if test="startTime!=null">
			and b.sendTime>=#{startTime,jdbcType=VARCHAR}
			<!-- <![CDATA[and a.updatedate >=  ${startTime}]]> -->
			</if>
			<if test="endTime!=null">
			  <![CDATA[and b.sendTime <= #{endTime,jdbcType=VARCHAR}]]>
			</if>
			and b.velocity >= #{velocity,jdbcType=INTEGER}
		 </where>) d order by d.sendTime
  </select>

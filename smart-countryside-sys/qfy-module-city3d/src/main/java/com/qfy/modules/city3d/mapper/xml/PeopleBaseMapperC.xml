  <select id="selectByExampleC" resultMap="BaseResultMap" parameterType="com.qfy.modules.city3d.po.PeopleBase" >
    select p.*,a.* from people_base p left join address a on p.add_id = a.add_id
    where LENGTH(substr(p.identity, 7 ,8))=8 and COALESCE(p.identity,'') like concat('%',#{identity},'%')
    and  COALESCE(p.name,'') like concat('%',#{name},'%') and  COALESCE(p.once_name,'') like concat('%',#{onceName},'%')
    and  COALESCE(p.gender,'') like concat('%',#{gender},'%') and  COALESCE(p.height,'') like concat('%',#{height},'%')
    and  COALESCE(p.blood_type,'') like concat(#{bloodType},'%') and  COALESCE(p.nation,'') like concat('%',#{nation},'%')
    and  concat(case is_important when '是' then '重点人口' end,';',case is_supervise when '是' then '监管对象' end
		,';',case is_drug when '是' then '吸毒人员' end,';',case is_visit when '是' then '上访人员' end
		,';',case is_mental when '是' then '精神病人员' end,';',case is_flow when '是' then '人口流动' end
		,';',case is_xinjiang when '是' then '内地涉疆人员' end,';',case is_out when '是' then '出境未归人员' end
		,';',case is_lost when '是' then '去向不明与无轨迹人员' end,';',case is_key when '是' then '重点人员' end
		) like concat('%',#{isDrug},'%')
	and  COALESCE(p.people_type,'') like concat('%',#{peopleType},'%')
	and  COALESCE(p.calture,'') like concat('%',#{calture},'%')
    and  COALESCE(p.tel,'') like concat('%',#{tel},'%') and  COALESCE(p.region,'') like concat('%',#{region},'%')
    and  COALESCE(p.political,'') like concat('%',#{political},'%') and  COALESCE(p.marriage,'') like concat('%',#{marriage},'%')
    and  COALESCE(p.unit_name,'') like concat('%',#{unitName},'%') and  COALESCE(p.job,'') like concat('%',#{job},'%')
    and  COALESCE(p.household_nature,'') like concat(#{householdNature},'%') and  COALESCE(p.household_address,'') like concat('%',#{householdAddress},'%')
    and  COALESCE(p.father_name,'') like concat('%',#{fatherName},'%') and  COALESCE(p.father_identity,'') like concat('%',#{fatherIdentity},'%')
    and  COALESCE(p.mother_name,'') like concat('%',#{motherName},'%') and  COALESCE(p.mother_identity,'') like concat('%',#{motherIdentity},'%')
    and  COALESCE(p.mate_name,'') like concat('%',#{mateName},'%') and  COALESCE(p.mate_identity,'') like concat('%',#{mateIdentity},'%')
    and  COALESCE(p.serve_army,'') like concat('%',#{serveArmy},'%')
    ORDER BY p.name
  </select>
  <select id="selectByExampleCPart" resultMap="BaseResultMap" parameterType="com.qfy.modules.city3d.po.PeopleBase" >
    select p.*,a.* from people_base p left join address a on p.add_id = a.add_id
    where LENGTH(substr(p.identity, 7 ,8))=8 and COALESCE(p.identity,'') like concat('%',#{identity},'%')
    or  COALESCE(p.name,'') like concat('%',#{name},'%')
    ORDER BY p.name
  </select>
  <select id="selectPeopleBaseById" resultMap="PbAPkPmPvhHpMap">
  	select *
	FROM people_base p left join address a on p.add_id = a.add_id
	left join house h on h.add_id = a.add_id
	left join house_panorama hp on hp.house_id = h.house_id
    where p.pp_id = #{ppId}
  </select>
  <select id="selectPeopleBaseByAddId" resultMap="BaseResultMap">
  	SELECT p.*, a.* FROM people_base p left join address a on p.add_id = a.add_id where p.add_id = #{addId}
  </select>
  <select id="queryPeopleByPolygon" resultMap="AddressResultMap">
 				select * from (select *, regexp_split_to_array(t.poi,',') as AAA from address t ) B
		where ST_Contains(ST_PolygonFromText('POLYGON((${polygon}))'),(ST_PointFromText('POINT('||AAA[1]||' '||AAA[2]||')')))
  </select>
  <delete id="batchDelete" parameterType="java.util.List" >
    delete from people_base where pp_id in
    <foreach collection="list" item="item" separator="," open="(" close=")">
    	#{item,jdbcType=VARCHAR}
    </foreach>
  </delete>
</mapper>

  <sql id="Example_Where_Clause" >
    <where >
      <foreach collection="oredCriteria" item="criteria" separator="or" >
        <if test="criteria.valid" >
          <trim prefix="(" suffix=")" prefixOverrides="and" >
            <foreach collection="criteria.criteria" item="criterion" >
              <choose >
                <when test="criterion.noValue" >
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue" >
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue" >
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue" >
                  and ${criterion.condition}
                  <foreach collection="criterion.value" item="listItem" open="(" close=")" separator="," >
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Update_By_Example_Where_Clause" >
    <where >
      <foreach collection="example.oredCriteria" item="criteria" separator="or" >
        <if test="criteria.valid" >
          <trim prefix="(" suffix=")" prefixOverrides="and" >
            <foreach collection="criteria.criteria" item="criterion" >
              <choose >
                <when test="criterion.noValue" >
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue" >
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue" >
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue" >
                  and ${criterion.condition}
                  <foreach collection="criterion.value" item="listItem" open="(" close=")" separator="," >
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Base_Column_List" >
    security_id, security_name, security_type, security_police_id, security_tel, security_describe,
    security_poi, security_center, security_video_id, equipment_id, security_community
  </sql>
  <select id="selectSecurityById" resultMap="BaseResultMap">
  	SELECT s.*, a.* FROM security s, address a WHERE s.add_id = a.add_id and s.security_id = #{sId}
  </select>
  <select id="getSecurityById" resultMap="BaseResultMap">
  	SELECT s.*, p.name FROM security s, police p WHERE s.security_police_id = p.police_id and s.security_id = #{sId}
  </select>
  <select id="selectByExampleC" resultMap="BaseResultMap" parameterType="com.qfy.modules.city3d.po.SecurityExample" >
    select p.*, s.* from security s, police p
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    and s.security_police_id = p.police_id
    <if test="orderByClause != null" >
      order by ${orderByClause}
    </if>
  </select>
  <select id="selectAll" resultMap="BaseResultMap">
    select p.name, s.* from security s left join police p
    on s.security_police_id = p.police_id
  </select>
  <delete id="batchDelete" parameterType="java.util.List" >
    delete from security where security_id in
    <foreach collection="list" item="item" separator="," open="(" close=")">
    	#{item,jdbcType=VARCHAR}
    </foreach>
  </delete>
</mapper>

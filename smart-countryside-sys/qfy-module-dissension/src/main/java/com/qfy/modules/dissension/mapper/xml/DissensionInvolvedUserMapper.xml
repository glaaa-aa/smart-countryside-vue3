<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qfy.modules.dissension.mapper.DissensionInvolvedUserMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE
		FROM  qfy_dissension_involved_user
		WHERE
			 tension_code = #{mainId} 	</delete>

	<select id="selectByMainId" parameterType="java.lang.String" resultType="com.qfy.modules.dissension.entity.DissensionInvolvedUser">
		SELECT *
		FROM  qfy_dissension_involved_user
		WHERE
			 tension_code = #{mainId} 	</select>
</mapper>

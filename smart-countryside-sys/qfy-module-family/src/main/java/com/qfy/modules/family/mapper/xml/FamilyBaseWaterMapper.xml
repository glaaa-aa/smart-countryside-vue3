<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qfy.modules.family.mapper.FamilyBaseWaterMapper">

    <select id="queryWater" parameterType="Object" resultType="com.qfy.modules.family.entity.FamilyBaseWater">
        select * from qfy_family_base_water where family_number = #{familyNumber} and year =#{year}
    </select>
</mapper>

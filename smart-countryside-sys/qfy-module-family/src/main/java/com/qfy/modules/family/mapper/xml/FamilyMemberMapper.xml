<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qfy.modules.family.mapper.FamilyMemberMapper">

    <delete id="deleteByMainId" parameterType="java.lang.String">
        DELETE
        FROM  qfy_family_member
        WHERE
            family_number = #{mainId}
    </delete>

    <select id="selectByMainId" parameterType="java.lang.String" resultType="com.qfy.modules.family.entity.FamilyMember">
        SELECT *
        FROM  qfy_family_member
        WHERE  family_number = #{mainId}
    </select>

</mapper>
